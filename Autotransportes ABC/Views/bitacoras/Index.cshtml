@model IEnumerable<Autotransportes_ABC.Models.bitacora>

@{
    ViewBag.Title = "Bitacoras";
}

<h2>Bitacoras</h2>

<p>
    @Html.ActionLink("Agregar nueva", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id_solicitud)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.folio)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.id_solicitud)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.folio)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.id_bitacora }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.id_bitacora }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.id_bitacora })
        </td>
    </tr>
}

</table>
